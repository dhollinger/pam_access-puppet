{
  "name": "dhollinger-pam",
  "version": "0.0.4",
  "author": "David Hollinger III",
  "summary": "Module for managing access.conf and limits.conf",
  "license": "Apache-2.0",
  "source": "https://github.com/dhollinger/puppet-module-pam",
  "project_page": "https://github.com/dhollinger/puppet-module-pam",
  "issues_url": "https://github.com/dhollinger/puppet-module-pam/issues",
  "operatingsystem_support": [
    {
